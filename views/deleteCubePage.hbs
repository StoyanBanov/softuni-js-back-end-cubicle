{{#with cubicle}}
<h1>Delete Cube</h1>
<div class="form">
    <form action="#" method="POST">
        <label for="name">Name</label>
        <input type="text" id="name" name="name" value="{{name}}" disabled="true" />
        <label for="description">Description</label>
        <textarea id="description" name="description" disabled="true" />{{description}}</textarea>
        <label for="image">ImageUrl</label>
        <input type="text" id="image" name="imageUrl" value="{{imageUrl}}" disabled="true" />
        <label for="difficulty">Difficulty</label>
        <select id="difficulty" name="difficultyLevel" disabled="true" />
        <option value="{{difficulty}}">{{difficultyText}}</option>
        </select>
        <input type="submit" value="Delete">
    </form>
</div>
{{/with}}